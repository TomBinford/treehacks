APP_ID=
WEBHOOK_SECRET=development
PRIVATE_KEY=

# Use `trace` to get verbose logging or `info` to show less
LOG_LEVEL=debug

# Go to https://smee.io/new set this to the URL that you are redirected to.
WEBHOOK_PROXY_URL=

# Warp API credentials for spawning agents
WARP_API_KEY=
WARP_ENVIRONMENT_ID=

# Arena UI URL (for generating links)
ARENA_UI_URL=http://localhost:3000

# Number of agents to spawn per job
NUM_AGENTS=1

WEBHOOK_PROXY_URL=
# Secrets for the GitHub App (you can run `npx create-probot-app <app-name>` elsewhere and copy variables from the generated .env)
APP_ID=
PRIVATE_KEY=
WEBHOOK_SECRET=

GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
WARP_API_KEY=
WARP_ENVIRONMENT_ID=

# GitHub App installation ID: you can find this in the URL when you open the app from your GitHub project's settings.
# It is unique to each installation of the app into a repo.
INSTALLATION_ID=

# A Vercel token and the team slug from the same account that your target repo is deployed to Vercel with.
VERCEL_TOKEN=
VERCEL_TEAM_SLUG=
