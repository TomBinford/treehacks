{
  "name": "treehacks",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app",
  "author": "Tom, Ritvik, Gavin",
  "license": "ISC",
  "homepage": "https://github.com//",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
